<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cc.tank.controller.OverController">
   <children>
      <ImageView fitHeight="649.0" fitWidth="960.0" layoutY="-4.0">
         <image>
            <Image url="@../image/gameOver.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="flag" fitHeight="405.0" fitWidth="395.0" layoutX="71.0" layoutY="141.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/defeated.png" />
         </image>
      </ImageView>
      <ImageView fx:id="toIndex" fitHeight="71.0" fitWidth="293.0" layoutX="667.0" layoutY="420.0" onMouseClicked="#mouseClickedToIndex" onMouseEntered="#mouseEnterToIndex" onMouseExited="#mouseExitToIndex">
         <image>
            <Image url="@../image/returnToIndex.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
